{
	"require": {
		"magento-hackathon/magento-composer-installer": "*",
		"connect20/mage_all_latest": "*"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.firegento.com"
		},
        {
            "type": "artifact",
            "url": "../artifact/"
        }
	],
	"extra":{
		"magento-deploystrategy": "copy",
		"magento-force": "override",
        "auto-append-gitignore": true,
		"magento-root-dir": "../htdocs/"
	}
}
